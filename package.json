{
  "name": "popfex-modernjs-plugin",
  "version": "0.1.0",
  "jsnext:source": "./src/index.ts",
  "main": "./dist/js/node/main.js",
  "scripts": {
    "prepare": "pnpm build && pnpm build:csmith",
    "prepublishOnly": "modern build --platform",
    "dev": "modern dev",
    "build": "modern build",
    "reset": "del-cli node_modules",
    "lint": "modern lint",
    "change": "modern change",
    "bump": "modern bump",
    "pre": "modern pre",
    "release": "modern release",
    "new": "modern new",
    "test": "modern test --passWithNoTests",
    "build:csmith": "csmith-tools build"
  },
  "engines": {
    "node": ">=14.17.6"
  },
  "eslintConfig": {
    "extends": [
      "@modern-js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet",
      "git add"
    ],
    "*.{js,jsx,mjs,mjsx,cjs,cjsx}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet",
      "git add"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ],
  "dependencies": {
    "@babel/runtime": "^7",
    "vm2": "^3.9.2"
  },
  "peerDependencies": {
    "react": "^17"
  },
  "devDependencies": {
    "react": "^17",
    "@modern-js/module-tools": "^1.1.5",
    "@modern-js/plugin-jarvis": "^1.0.0",
    "@modern-js/plugin-testing": "^1.0.0",
    "del-cli": "^4.0.1",
    "typescript": "^4",
    "@types/jest": "^26.0.9",
    "@types/node": "^14",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@modern-js/generator-plugin": "^1.0.0",
    "@modern-js/codesmith-tools": "^1.0.7"
  },
  "sideEffects": [
    "*.css",
    "*.less",
    "*.sass",
    "*.scss"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "modernConfig": {
    "output": {
      "packageMode": "node-js"
    }
  },
  "files": [
    "/templates",
    "/dist/js/node/main.js"
  ],
  "meta": {
    "extend": "mwa"
  }
}